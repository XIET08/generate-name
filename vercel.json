{
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" },
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "*.css", "use": "@vercel/static" },
    { "src": "*.js", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/generate-names", "dest": "server.js" },
    { "src": "/(.*)", "dest": "/$1" }
  ],
  "env": {
    "API_KEY": "@api_key",
    "API_ENDPOINT": "@api_endpoint"
  }
}